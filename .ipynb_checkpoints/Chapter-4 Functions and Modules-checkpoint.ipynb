   "source": [
    "# Function \n",
    "# Return the harmonic mean of the number in the interval [100,200] which is divisible by  a = 9\n",
    "\n",
    "def hm(a):\n",
    "    'Return the harmonic mean'\n",
    "    h, b = 0, 100+a-100%a\n",
    "    while b<200:\n",
    "        h +=1/b\n",
    "        b+=a\n",
    "    return 1/h\n",
    "\n"
   ]
  },
